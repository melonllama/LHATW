        x+=sign(hsp);
    };
    hsp = 0;
};
x+=hsp;

if (place_meeting(x,y+vsp,Wall_obj)){
    while(!place_meeting(x,y+sign(vsp),Wall_obj)) {
        y+=sign(vsp);
    };
    vsp = 0;
};
y+=vsp;







</string>
